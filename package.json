{
  "name": "reactive-color",
  "version": "1.0.0",
  "scripts": {
    "start": "parcel index.html --open",
    "build": "parcel build index.html",
    "test": "jest src --watch",
    "lint": "xo"
  },
  "dependencies": {
    "core-js": "^3.3.1",
    "eslint-config-tram-one": "^4.0.0",
    "jest": "^24.8.0",
    "jest-transform-stub": "^2.0.0",
    "parcel-bundler": "^1.6.1",
    "parcel-plugin-nuke-dist": "^1.0.0",
    "regenerator-runtime": "^0.13.3",
    "sass": "^1.20.1",
    "tram-blueprints": "^1.0.0",
    "tram-one": "^8.1.1",
    "xo": "^0.23.0"
  },
  "keywords": [
    "tram-one"
  ],
  "babel": {
    "presets": [
      "@babel/preset-env"
    ]
  },
  "jest": {
    "transform": {
      ".+\\.(css|styl|less|sass|scss|png|jpg|ttf|woff|woff2)$": "jest-transform-stub",
      "^.+\\.js$": "babel-jest"
    }
  },
  "xo": {
    "extends": [
      "tram-one"
    ]
  }
}
